{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0.0",
  "permissions": ["storage"],
  "action": { "default_popup": "src/popup/index.html" },
  "background": { "service_worker": "src/background/background.js" },
  "content_scripts": [{ "matches": ["<all_urls>"], "js": ["src/content/content.js"] }]
}
